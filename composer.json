{
    "name": "ferror/phpscan",
    "license": "MIT",
    "version": "1.0.0",
    "description": "Package scans projects source directory to check whether it uses proper layered namespaces",
    "type": "library",
    "autoload" : {
        "psr-4" : {
            "Ferror\\Bundle\\PHPScan\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Ferror\\Bundle\\PHPScan\\" : "tests/"
        }
    },
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
